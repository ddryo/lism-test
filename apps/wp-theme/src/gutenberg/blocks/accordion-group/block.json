{
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "lism-blocks/accordion-group",
	"title": "Accordion Group",
	"category": "lism-blocks",
	"supports": {
		"anchor": true,
		"className": false,
		"align": ["wide", "full"],
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"hasDivider": {
			"type": "boolean",
			"default": false
		},
		"allowMultiple": {
			"type": "boolean",
			"default": true
		},
		"duration": {
			"type":"number"
		}
	},
	"editorScript": "file:./index.js",
	"textdomain": "lism-blocks"
}
