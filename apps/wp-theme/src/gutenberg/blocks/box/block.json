{
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "lism-blocks/box",
	"title": "Box",
	"category": "lism-blocks",
	"keywords": ["group"],
	"supports": {
		"anchor": true,
		"className": false,
		"align": ["wide", "full"],
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"templateLock": {
			"type": ["string", "boolean"],
			"enum": ["all", "insert", "contentOnly", false]
		},
		"tagName": {
			"type": "string",
			"default": "div"
		},
		"flowGap": {
			"type": "string"
		},
		"textAlign": {
			"type": "string"
		},
		"lismProps": {
			"type": "array",
			"items": {
				"type": "object"
			},
			"default": []
		}
	},
	"editorScript": "file:./index.js",
	"style": "lism-blocks-container-style",
	"textdomain": "lism-blocks"
}
