{
	"apiVersion": 3,
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"name": "lism-blocks/center",
	"title": "Center",
	"category": "lism-blocks",
	"keywords": ["group"],
	"supports": {
		"anchor": true,
		"className": false,
		"align": ["wide", "full"],
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		}
	},
	"attributes": {
		"justifyContent": {
			"type": "string",
			"default": "flex-start"
		},
		"height": {
			"type": "string"
		},
		"size": {
			"type": "string",
			"enum": ["cover", "content"]
		}
	},
	"editorScript": "file:./index.js",
	"textdomain": "lism-blocks"
}
